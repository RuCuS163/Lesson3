def div(*args):
    try:
        a = int(input("Введите делимое: "))
        b = int(input("Введите делитель: "))
        res = a / b
    except ValueError:
        return 'Value error'
    except ZeroDivisionError:
        return "Ошибка, на ноль делить нельзя!"
    return res
print(f'Результат деления = {div()}')
